<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Resolver -->
    <color name="resolver_list_bg">@android:color/white</color>

    <!-- Immersive cling -->
    <color name="immersive_cling_bg_color">@color/accent_device_default_light</color>
    <color name="immersive_cling_text_color">@android:color/white</color>
    <color name="immersive_cling_button_text_color">@android:color/white</color>

    <!-- Toast text and bg colors -->
    <color name="toast_bg_color">#E6EEEEEE</color>
    <color name="toast_text_color">@color/primary_text_default_material_light</color>

    <!-- Fastscroller -->
    <color name="fastscroller_text_color">#FFFFFFFF</color>

    <!-- Two target preference vertical divider color -->
    <color name="list_divider_dark">#64000000</color>

    <!-- Auto fill -->
    <color name="autofill_save_title_color">@color/primary_text_default_material_light</color>
    <color name="autofill_autofill_save_custom_subtitle_color">@color/primary_text_default_material_light</color>

    <!-- Settings icon tint color -->
    <color name="settings_icon_tint_color">#737373</color>

    <!-- tooltip light text color -->
    <color name="tooltip_text_light">@color/foreground_material_light</color>
    <!-- tooltip dark text color -->
    <color name="tooltip_text_dark">@color/foreground_material_dark</color>

    <!-- App permission tint -->
    <color name="app_permission_icon_tint">@android:color/black</color>

    <!-- Notification icon tint -->
    <color name="notification_icon_default_color">@android:color/notification_default_color</color>
    <!-- Ambient notification icon tint -->
    <color name="notification_ambient_icon_default_color">@color/notification_icon_default_color</color>

    <!-- chooser_service_row_background_color for devices with exposures.
        See also Theme.DeviceDefault.Resolver.Exposed. -->
    <color name="chooser_service_row_background_color_exposed">@color/chooser_service_row_background_color</color>
</resources>
